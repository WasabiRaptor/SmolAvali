  },


    "walkSpeed" : 6.0,
    "runSpeed" : 14.0,

    "bounceFactor" : 0.0,
  "stopOnFirstBounce" : false,

  "ignorePlatformCollision" : false,
  "maximumPlatformCorrection" : 0.12,
  "maximumPlatformCorrectionVelocityFactor" : 0.09
  }
